#!/home/yaki/github_projects/interplanetary/kupcake/contracts-kupcake/contracts/presale/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vyper==0.2.12','console_scripts','vyper-serve'
__requires__ = 'vyper==0.2.12'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vyper==0.2.12', 'console_scripts', 'vyper-serve')()
    )
